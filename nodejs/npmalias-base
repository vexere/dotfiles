# print prefix for npm alias
alias npmprefix="echo $NPM_ALIAS_PREFIX"

alias "$NPM_ALIAS_PREFIX"="pnpm"
alias "$NPM_ALIAS_PREFIX"i="pnpm install"
alias "$NPM_ALIAS_PREFIX"id="pnpm install --save-dev"
alias "$NPM_ALIAS_PREFIX"ig="pnpm install -g"
alias "$NPM_ALIAS_PREFIX"r="pnpm run-script"
alias "$NPM_ALIAS_PREFIX"t="pnpm test"
alias "$NPM_ALIAS_PREFIX"it="pnpm install && npm test"
alias "$NPM_ALIAS_PREFIX"is="pnpm install && npm start"
alias "$NPM_ALIAS_PREFIX"its="pnpm install && npm test && npm start"
alias "$NPM_ALIAS_PREFIX"s="pnpm start"
alias "$NPM_ALIAS_PREFIX"lk="pnpm link"
alias "$NPM_ALIAS_PREFIX"w="pnpm run watch"
